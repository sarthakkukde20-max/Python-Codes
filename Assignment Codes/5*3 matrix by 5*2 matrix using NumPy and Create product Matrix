import numpy as np

print("Enter elements for 5x3 matrix:")
A = []

for i in range(5):
    row = input().split()
    row = [int(x) for x in row]
    A.append(row)

print("Enter elements for 3x2 matrix:")
B = []

for i in range(3):
    row = input().split()
    row = [int(x) for x in row]
    B.append(row)

A = np.array(A)
B = np.array(B)

C = A @ B

print("Product Matrix:")
print(C)

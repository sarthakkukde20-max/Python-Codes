for i in range(1,6):
  for j in range(i):
    print(ch(65+j,end="")
  print()


for i in range(1,5):
  for j in range(i):
    if j%2==0:
      print("*",end="")
    else:
      print("#",end="")
  print()


char=['p','y','t','b','o']
for i in range(len(char)):
  print(char[i]*(i+1))


word="python"
for i in range(1,len(word)):
  print(word[:i])
